{
  "name": "Contest Tracker Pro",
  "version": "1.0.0",
  "description": "Track competitive programming contests from LeetCode, CodeForces, CodeChef, AtCoder, HackerEarth, HackerRank, and KickStart - all in one place",
  "author": "Team Innoreva",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/logo16.png",
      "48": "images/logo48.png",
      "128": "images/logo128.png"
    }
  },
  "icons": {
    "16": "images/logo16.png",
    "48": "images/logo48.png",
    "128": "images/logo128.png"
  },
  "permissions": ["storage"],
  "host_permissions": [
    "https://www.kontests.net/*",
    "https://kontests.net/*"
  ],
  "offline_enabled": false
}
